{
    "extends": ["eslint:recommended"],
    "plugins": [ "jsdoc" ],
    "env": {
      "node": true,
      "es2021": true
    },
    "ignorePatterns": [ "scripts/util/secret.js" ],
    "globals": {},
    "rules": {
      "no-unused-vars": "warn",
      "no-empty": "off",
      "block-spacing": "warn",
      "arrow-spacing": "warn",
      "space-before-blocks": "warn",
      "keyword-spacing": "warn",
      "no-irregular-whitespace": "off",
      "no-class-assign": "off",
      "jsdoc/no-undefined-types": 1
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    }
}
